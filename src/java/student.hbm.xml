<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="com.psx.source.Student" table="details">
      <id name="id" type="long" column="id">
            <generator class="native" />
            <!-- geneartor class ="native" defines auto increment -->
        </id>
        <property name="name" type="string" not-null="true" length="30" column="name" />
        <property name="address" type="string" not-null="true" length="60" column="address" />
  </class>
</hibernate-mapping>
